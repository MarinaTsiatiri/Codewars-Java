public class Codewars {
  public static String oddOrEven (int[] array) {
    boolean found = false;
    int sum = 0;
    
    for (int i : array) {
      sum += i;

    }
    
    if (sum%2!=0){
      found = true;
    }
    
    if (found) {
      return "odd";
    } else {
      return "even";
    }
  }
}
